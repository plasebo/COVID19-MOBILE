    "name": "Covid19-MU",
    "framework": "vue",
    "template": "tabs",
    "cssPreProcessor": "scss",
    "bundler": "webpack",
    "webpack": {
      "developmentSourceMap": true,
      "productionSourceMap": true,
      "hashAssets": false,
      "preserveAssetsPaths": false,
      "inlineAssets": true
